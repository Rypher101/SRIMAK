@model SRIMAK.Models.RoutModel

@{
    ViewData["Title"] = "EditRout";
    Layout = "~/Views/Shared/_LayoutManagerDashboard.cshtml";
}

<div class="row">
<div class="col-md-12">
<div class="card ">
<div class="card-header ">
    <h4 class="card-title">Edit Rout</h4>
</div>
<div class="card-body">
    <hr />
    <div class="row">
        <div class="col-md-4 offset-4">
            <form asp-action="EditRoutResult">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-group">
                    <label asp-for="RoutId" class="control-label"></label>
                    <input asp-for="RoutId" class="form-control" />
                    <span asp-validation-for="RoutId" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="Town" class="control-label"></label>
                    <input asp-for="Town" class="form-control" />
                    <span asp-validation-for="Town" class="text-danger"></span>
                </div>
                <input name="Rout2" id="Rout2" value="@Model.RoutId" hidden="" />
                <div class="form-group">
                    <input type="submit" value="Save" class="btn btn-primary" />
                </div>

            </form>
        </div>
    </div>

</div>
</div>
</div>
</div>


